{
  "dashboard": {
    "id": null,
    "title": "arifOS Trinity MCP Server",
    "tags": ["arifos", "mcp", "constitutional"],
    "timezone": "browser",
    "panels": [
      {
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(arifos_requests_total[5m])",
            "legendFormat": "{{tool}}"
          }
        ]
      },
      {
        "title": "Verdict Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "arifos_verdicts_total",
            "legendFormat": "{{verdict}}"
          }
        ]
      },
      {
        "title": "Response Latency (p99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(arifos_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ]
      },
      {
        "title": "Floor Violations",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(arifos_floor_violations_total[5m])",
            "legendFormat": "{{floor}}"
          }
        ]
      },
      {
        "title": "Rate Limit Hits",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(arifos_rate_limit_hits_total[5m])",
            "legendFormat": "{{tool}} ({{limit_type}})"
          }
        ]
      },
      {
        "title": "Active Sessions",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "arifos_active_sessions",
            "legendFormat": "Active"
          }
        ]
      },
      {
        "title": "Ledger Entries",
        "type": "graph",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 16},
        "targets": [
          {
            "expr": "rate(arifos_ledger_entries_total[5m])",
            "legendFormat": "{{verdict}}"
          }
        ]
      }
    ],
    "refresh": "10s",
    "schemaVersion": 38
  }
}
